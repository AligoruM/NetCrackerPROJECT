<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:library="urn:import:catalogApp.client.view.mainPage.library">
    <g:HTMLPanel>
        <g:DecoratorPanel>
            <g:HorizontalPanel>
                <g:DecoratorPanel>
                    <g:VerticalPanel>
                        <library:tabs.BookTabView ui:field="bookView"/>
                        <g:Button ui:field="deleteBooksButton" text="Delete books from lib"/>
                    </g:VerticalPanel>
                </g:DecoratorPanel>
                <g:DecoratorPanel>
                    <g:VerticalPanel>
                        <library:tabs.SongTabView ui:field="songView"/>
                        <g:Button ui:field="deleteSongsButton" text="Delete songs from lib"/>
                    </g:VerticalPanel>
                </g:DecoratorPanel>
            </g:HorizontalPanel>
        </g:DecoratorPanel>
    </g:HTMLPanel>
</ui:UiBinder>