<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <g:HTMLPanel>
        <g:VerticalPanel>
            <g:HorizontalPanel>
                <g:Button ui:field="addButton" text="Add item"/>
                <g:Button ui:field="addToLib" text="Add item to lib"/>
                <g:Button ui:field="editItem" text="Edit item"/>
                <g:Button ui:field="archiveButton" text="To archive"/>
                <g:Button ui:field="restoreButton" text="Restore"/>
            </g:HorizontalPanel>
            <g:TabPanel ui:field="tabPanel"/>
        </g:VerticalPanel>
    </g:HTMLPanel>
</ui:UiBinder>

