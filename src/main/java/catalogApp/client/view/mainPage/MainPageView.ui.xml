<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel>
        <g:HorizontalPanel width="100%">
            <g:HorizontalPanel width="400px" height="80px">
                <g:ToggleButton ui:field="profileButton" width="100px" height="70px" text="Profile"/>
                <g:ToggleButton ui:field="libButton" width="100px" height="70px" text="Library"/>
                <g:ToggleButton ui:field="settingButton" width="100px" height="70px" text="Settings"/>
                <g:ToggleButton ui:field="userButton" width="100px" height="70px" text="Users"/>
            </g:HorizontalPanel>
            <g:cell horizontalAlignment="ALIGN_RIGHT">
                <g:HTMLPanel ui:field="profileBarPanel"/>
            </g:cell>
        </g:HorizontalPanel>
        <g:SimplePanel ui:field="mainPanel" width="100%" height="100%"/>

    </g:HTMLPanel>
</ui:UiBinder>